generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

//modelo que vai ser usado no nosso banco de dados, isso é só um exemplo

model Equipe {
  id    String @id @default(auto()) @map("_id") @db.ObjectId //id da equipe
  nome String 
 
  criadoEm DateTime
  
}